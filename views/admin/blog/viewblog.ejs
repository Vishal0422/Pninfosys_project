<%- include('../../header.ejs') %>
<section class="login-wrap mtb-40">
    <div class="container">
        <div class="row justify-content-center">
            <div class="col-md-8">
                <div class="login-box">
                    <h1>Display data</h1>
                    <table class="table table-bordered text-black">
                        
                        <tr>
                            <th>No.</th>
                            <th>Id</th>
                          <th>Title</th>
                          <th>Description</th>
                          
                        </tr>
                        <% var i=1 %>
                        
                            <!-- console.log(item) -->
                            <tr>
                                <td><%= i++ %></td>
                                <td><%= data._id %></td>
                                <td><%= data.title %></td>
                                <td><%= data.description %></td>
                                
                              </tr>
                       
                        
                        
                      </table>
                </div>         
            </div>
        </div>
    </div>
</section>
<%- include('../../footer.ejs') %>